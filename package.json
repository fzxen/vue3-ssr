{
  "name": "vue-ssr",
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node ./src/server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "prebuild": "vuedx-typecheck .",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry_server.ts --outDir dist/server",
    "preserver": "npm run build",
    "server": "cross-env NODE_ENV=production ts-node ./src/server/index.ts"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/koa": "^2.11.8",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/server-renderer": "^3.0.5",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "cross-env": "^7.0.3",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.1",
    "koa-connect": "^2.1.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "sass": "^1.32.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0"
  }
}
